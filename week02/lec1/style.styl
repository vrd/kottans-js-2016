input[type=radio]
  display none
  
label
  margin 10px
  padding 4px
  border 1px solid #E8E9EE
  border-radius 5px
  background #F7F7FA
  display inline-flex
  align-items center
  justify-content center
  font-size 14px
  
input[type=radio]:checked + label
  padding 0px
  border 5px solid #76A7E1
  position relative
  
input[type=radio]:checked + label::after
  display block
  position absolute
  top -16px
  right -16px
  content "L"
  font-size 18px
  font-weight bold
  transform scaleX(-1) rotate(-45deg)
  width 28px
  height 28px
  border-radius 14px
  background #76A7E1
  color #fff
  text-align center
  line-height 26px
   

input[type=range] {
  -webkit-appearance none  
  width 80%
  z-index 0
  position relative
}
input[type=range]::before {
  margin-right -8px
  z-index 1
  position relative
  display block
  content ""
  width 8px
  height 8px
  border-radius 4px
  background #D6D8E2  
}
input[type=range]::after {
  margin-left -8px  
  z-index 1
  position relative
  display block
  content ""
  width 8px
  height 8px
  border-radius 4px
  background #D6D8E2
}
input[type=range]:focus {
  outline none
}

input[type=range]::-webkit-slider-runnable-track {
  width 100%
  height 8px
  cursor pointer
  animate 0.2s
  background #EFF0F5
  border-radius 4px  
}
input[type=range]::-webkit-slider-thumb {
  height 18px
  width 18px
  position relative
  z-index 2
  border-radius 10px
  background #EDBE76
  cursor pointer
  -webkit-appearance none
  margin-top -5px
}

input[type=range]::-moz-range-track {
  width: 100%;
  height: 8px;
  cursor: pointer;
  animate: 0.2s;  
  background: #EFF0F5;
  border-radius: 4px;
  position: relative;
  z-index: 0;
}
input[type=range]::-moz-range-thumb {
  height: 18px;
  width: 18px;
  border-radius: 9px;
  background: #EDBE76;
  cursor: pointer;
  position: relative;
  z-index: 2;
  border-color: transparent;
}

input[type=range]::-ms-track {
  width: 100%;
  height: 8px;
  cursor: pointer;
  animate: 0.2s;
  background: transparent;
  border-color: transparent;
  border-width: 16px 0;
  color: transparent;
}
input[type=range]::-ms-fill-lower {
  background: #EFF0F5; 
  border-radius: 4px;  
}
input[type=range]::-ms-fill-upper {
  background: #EFF0F5;  
  border-radius: 4px;  
}
input[type=range]::-ms-thumb {
  height: 18px;
  width: 16px;
  position: relative;
  z-index: 2;
  border-radius: 9px;
  background: #EDBE76;
  cursor: pointer;
  margin-top: 0px;
  border-color: transparent;
}
input[type=range]:focus::-ms-thumb {
border-color: transparent;
}
input[type=range]:focus::-ms-fill-lower {
  background: #EFF0F5;
}
input[type=range]:focus::-ms-fill-upper {
  background: #EFF0F5;
}

      
